{
  "name": "vueexpress-syncfusionfilemanager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:backend": "npm --prefix backend start",
    "build": "npm install && npm --prefix frontend install && npm --prefix backend install && npm --prefix frontend run build",
    "start:frontend": "npm --prefix frontend run",
    "start:backend": "npm --prefix backend start",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "preview": "npm --prefix frontend run preview"
  },
  "dependencies": {
    "@syncfusion/ej2-base": "^28.1.33",
    "@syncfusion/ej2-vue-filemanager": "^28.1.33",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^6.37.5",
    "vue": "^3.5.13",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.32",
    "@vitejs/plugin-vue": "^5.2.1",
    "concurrently": "^9.1.0",
    "vite": "^6.0.1"
  }
}
